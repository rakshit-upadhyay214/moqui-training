<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/service-definition-3.xsd">
    <service verb="create" noun="MoquiTraining">
        <in-parameters>
            <auto-parameters entity-name="org.moqui.training.MoquiTraining"/>
            <parameter name="trainingId" required="true"/>
            <parameter name="trainingName"/>
            <parameter name="trainingDate" type="Date"/>
            <parameter name="trainingPrice"/>
            <parameter name="trainingDuration"/>
        </in-parameters>
        <out-parameters>
            <auto-parameters include="pk" required="true"/>
        </out-parameters>
        <actions>
            <service-call name="org.moqui.training.MoquiTraining.create#MoquiTraining" in-map="context" out-map="context"/>
        </actions>
    </service>
</services>
